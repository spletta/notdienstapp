<%= render 'shared/error_messages', object: f.object %>
<%= f.label "First Name" %>
<%= f.text_field :name %>

<%= f.fields_for :accounts do |account_form| %>
  <%= render :partial => 'shared/account', :locals => { :form => account_form } %>
<% end %>

<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :password %>
<%= f.password_field :password %>

<%= f.label :password_confirmation do %>
  <%= t "confirm.password", default: "Confirm Password" %>
<% end %>
<%= f.password_field :password_confirmation %>

<br />